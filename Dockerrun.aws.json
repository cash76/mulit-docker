{
    "AWSEBDockerfunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "cash76/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "cash76/mulit-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "cash76/mulit-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "cash76/mulit-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["clinet", "server"]
        }
    ]
}